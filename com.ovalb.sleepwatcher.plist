<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>KeepAlive</key>
	<false/>
	<key>Label</key>
	<string>com.ovalb.sleepwatcher</string>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/local/sbin/sleepwatcher</string>
		<string>-V</string>
		<string>--sleep</string>
		<string>/usr/local/sbin/disc_bluetooth</string>
	</array>
	<key>StandardOutPath</key>
	<string>/usr/local/sbin/disc_bluetooth.logs</string>
	<key>StandardErrorPath</key>
	<string>/usr/local/sbin/disc_bluetooth_err.logs</string>
	<key>RunAtLoad</key>
	<true/>
</dict>
</plist>
